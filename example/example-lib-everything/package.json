{
  "name": "example-lib-everything",
  "description": "An everything example",
  "version": "3.3.0",
  "private": true,
  "devDependencies": {
    "mrbuilder": "^3.3.0"
  },
  "dependencies": {
    "mrbuilder-plugin-env": "^3.3.0",
    "mrbuilder-plugin-graphql": "^3.3.0",
    "mrbuilder-plugin-lodash": "^3.3.0",
    "mrbuilder-plugin-markdown": "^3.3.0",
    "mrbuilder-plugin-moment": "^3.3.0",
    "mrbuilder-plugin-stylus": "^3.3.0",
    "mrbuilder-plugin-worker": "^3.3.0",
    "mrbuilder-plugin-yaml": "^3.3.0"
  },
  "scripts": {
    "analyze": "mrbuilder",
    "prepublish": "mrbuilder",
    "start": "mrbuilder",
    "test": "mrbuilder",
    "karma": "mrbuilder"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "source": "src",
  "main": "lib",
  "mrbuilder": {
    "presets": [
      "mrbuilder-preset-lib"
    ],
    "plugins": [
      [
        "mrbuilder-plugin-env",
        {
          "environment": [
            "NODE_ENV",
            "USER"
          ]
        }
      ],
      "mrbuilder-plugin-yaml",
      "mrbuilder-plugin-markdown",
      "mrbuilder-plugin-lodash",
      "mrbuilder-plugin-env",
      "mrbuilder-plugin-moment",
      "mrbuilder-plugin-stylus",
      "mrbuilder-plugin-worker",
      "mrbuilder-plugin-typescript",
      "mrbuilder-plugin-graphql"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "mrbuilder-plugin-html",
            false
          ]
        ]
      },
      "test": {
        "plugins": [
          [
            "mrbuilder-plugin-css",
            {
              "useStyleLoader": true
            }
          ]
        ]
      }
    }
  },
  "license": "0BSD"
}
