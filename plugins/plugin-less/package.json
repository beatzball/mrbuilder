{
  "name": "@mrbuilder/plugin-less",
  "version": "4.0.0-alpha.4",
  "description": "less support for mrbuilder",
  "dependencies": {
    "@mrbuilder/plugin-css": "^4.0.0-alpha.4",
    "less": "^3.10.3",
    "less-loader": "^5.0.0"
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "scripts": {
    "test": "mrbuilder-mocha"
  },
  "source": "src",
  "main": "src",
  "mrbuilder": {
    "options": {
      "test": "/.less$",
      "modules": true,
      "options": {
        "strictMath": true,
        "noIeCompat": true
      }
    },
    "plugins": [
      "@mrbuilder/plugin-css"
    ]
  },
  "license": "0BSD",
  "gitHead": "771c5d4ad70156a7c7e8730c68369ed872e2cfec",
  "publishConfig": {
    "access": "public"
  }
}
