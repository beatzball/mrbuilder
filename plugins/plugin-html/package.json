{
  "name": "@mrbuilder/plugin-html",
  "version": "4.0.0-alpha.4",
  "description": "html plugin for mrbuilder",
  "dependencies": {
    "@mrbuilder/plugin-babel": "^4.0.0-alpha.4",
    "@mrbuilder/utils": "^4.0.0-alpha.4",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0"
  },
  "source": "src",
  "main": "src",
  "license": "0BSD",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "gitHead": "771c5d4ad70156a7c7e8730c68369ed872e2cfec",
  "publishConfig": {
    "access": "public"
  },
  "mrbuilder": {
    "options": {
      "publicPath": "public",
      "filename": "[name].html",
