{
  "name": "@mrbuilder/plugin-jest",
  "version": "4.0.0-alpha.3",
  "bin": {
    "mrbuilder-jest-cli": "./bin/cli.js"
  },
  "description": "Provides mrbuilder wrapper for jest",
