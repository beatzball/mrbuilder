{
  "name": "@mrbuilder/plugin-react-styleguidist",
  "version": "4.0.0-alpha.4",
  "description": "Mrbuilder Plugin for styleguidist",
  "main": "src",
  "dependencies": {
    "@mrbuilder/plugin-doc-prop-types": "^4.0.0-alpha.4",
    "@mrbuilder/plugin-webpack": "^4.0.0-alpha.4",
    "react-styleguidist": "^10.2.0"
  },
  "mrbuilder": {
    "plugins": [
      [
        "@mrbuilder/plugin-hot",
        false
      ],
      "@mrbuilder/plugin-doc-prop-types"
    ],
    "env": {
      "styleguide": {
        "plugins": [
          [
            "@mrbuilder/plugin-html",
            false
          ],
          "@mrbuilder/plugin-doc-prop-types",
          [
            "@mrbuilder/plugin-webpack",
            {
              "demo": "./styleguide"
            }
          ],
          "@mrbuilder/plugin-react-styleguidist"
        ]
      }
    }
  },
  "license": "0BSD",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "gitHead": "771c5d4ad70156a7c7e8730c68369ed872e2cfec",
  "publishConfig": {
    "access": "public"
  }
}
